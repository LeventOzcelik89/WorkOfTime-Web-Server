@model Infoline.WorkOfTime.BusinessData.VWSH_Ticket
@{
    ViewBag.Title = "Kullanıcı Giriş Detayı";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-horizontal" data-selector="modalContainer">


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md">Kullanıcı</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.User_Title, new Dictionary<string, object>()
            {
                {"class", "form-control"},
                {"readonly","readonly"}
            })
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md">Giriş Tarihi</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.createtime, new Dictionary<string, object>()
            {
                {"class", "form-control"},
                {"readonly","readonly"}
            })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md">Çıkış Tarihi</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.endtime, new Dictionary<string, object>()
            {
                {"class", "form-control"},
                {"readonly","readonly"}
            })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md">IP Adresi</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.IP, new Dictionary<string, object>()
            {
                {"class", "form-control"},
                {"readonly","readonly"}
            })
        </div>
    </div>


    <div class="buttons">
        <button class="btn btn-md btn-danger pull-left" data-task="modalClose">Geri</button>
    </div>

</div>

