<link href="~/Content/Custom/MyPage/css/General.css" rel="stylesheet" />
<script type="text/javascript">

    var myPage = {
        days: ["Pazartesi", "Salı", "Çarşamba", "Perşembe", "Cuma"],
        allData: $.ajax({
            url: '/Student/GetAllDataByStudent',
            data: null,
            type: "POST",
            dataType: "JSON",
            async: false
        }).responseJSON,
        functions: {
            getEventCountForRange: function (startSlot, endSlot, rowsCount) {
                var scheduler = $(startSlot.element).closest("[data-role=scheduler]").getKendoScheduler();
                var currentTimezoneOffset = kendo.date.MS_PER_MINUTE * new Date().getTimezoneOffset();
                var rangeStart = new Date(startSlot.start + currentTimezoneOffset);
                var rangeEnd = new Date(endSlot.end + currentTimezoneOffset);

                return scheduler.occurrencesInRange(rangeStart, rangeEnd).length - rowsCount + 1;
            },
            createScheduler: function (element, data) {
                $("#" + element).kendoScheduler({
                    date: new Date(),
                    startTime: new Date(),
                    endTime: new Date().addDays(1),
                    height: 450,
                    views: ["agenda"],
                    editable: false,
                    eventTemplate: $("#event-template").html(),
                    dataSource: new kendo.data.SchedulerDataSource({
                        data: data
                    })
                })
            },
            loadMyLessons: function () {

                $.each($.Enumerable.From(myPage.allData.dersler).Where(a => a.sezon == "2020-2021").ToArray(), function (i, lesson) {
                    var elem = '<li class="warning-element" id="task23">' +
                        '    <h3>' + lesson.ders_adi + '( ' + lesson.ders_kodu + ' )</h3>' +
                        '    <div class="agile-detail">' +
                        '<span class="badge badge-success pull-right" style="font-size:13px;"> ' + lesson.kredi + ' Kredi</span>' +
                        '        <p>Hoca : ' + lesson.hoca_adi + '</p>' +
                        '    </div>' +
                        '</li>';

                    $('#mylessons').append(elem);
                })
            },
            loadNotification: function () {
                var data = myPage.allData.duyuru;
                $.each(data, function (i, item) {
                    var elem = '<div class="vertical-timeline-block">' +
                        '<div class="vertical-timeline-icon blue-bg">' +
                        '    <i class="fa fa-file-text"></i>' +
                        '</div>' +
                        '<div class="vertical-timeline-content" style="background-color:#f3f3f3;">' +
                        '    <h2>' + item.konu + '</h2>' +
                        '    <p>' + item.detay + ' </p>' +
                        '</div>' +
                        '</div>';
                    $('#vertical-timeline').append(elem);
                })
            },
            loadExamResult: function () {
                var data = myPage.allData.sinavSonuc;

                $.each(data, function (i, item) {

                    var sinavlar = "";

                    $.each(item.sinavlar, function (j, sinav) {
                        '<span class="badge badge-success pull-right" style="font-size:13px;"> 2 Kredi</span>'
                        sinavlar += '<p>' + sinav.sinav_adi + ' : <span class="badge badge-success" style="font-size:13px;"> ' + sinav.puan + '</span></p>';
                    })

                    var elem = '<li class="warning-element" id="task23">' +
                        '    <h3>' + item.ders_adi + ' (' + item.ders_kodu + ') </h3>' +
                        sinavlar +
                        '</li>';

                    $('#myresults').append(elem);
                })

            },
            loadSchedule: function () {
                for (var i = 1; i <= myPage.days.length; i++) {
                    var data = $.Enumerable.From(myPage.allData.dersProgrami).Where(a => a.gun == i).ToArray();

                    if (data.length > 0) {
                        var list = $('<ul>').addClass("todo-list m-t ui-sortable");
                        $.each(data, function (j, item) {
                            var text = '    <li>' +
                                '<div class="icheckbox_square-green" style="position: relative;">' +
                                '<input type="checkbox" value="" name="" class="i-checks" style="position: absolute; opacity: 0;">' +
                                '<ins class="iCheck-helper" style="position: absolute; top: 0%; left: 0%; display: block; width: 100%; height: 100%; margin: 0px; padding: 0px; background: rgb(255, 255, 255); border: 0px; opacity: 0;"></ins>' +
                                '</div>' +
                                '<small class="label label-success"><i class="fa fa-clock-o"></i> ' + item.baslangic_saat + "-" + item.bitis_saat + '</small>' +
                                '<span class="m-l-xs">' + item.ders_adi + " (" + item.ders_kodu + ")" + '</span>' +
                                '</li>';

                            list.append(text);
                        })

                        $('#dersProgram').append($('<h2>').text(myPage.days[i - 1])).append(list);
                    }
                }
            },
            transkriptRequest: function () {
               swal({
                title: "Devam Et ?",
                text: "Transkript talebi yapmak istediğinize emin misiniz ?",
                type: "warning",
                content: "input",
                showCancelButton: true,
                confirmButtonColor: "#DD6B55",
                confirmButtonText: "Evet",
                cancelButtonText: "Hayır",
                closeOnConfirm: false,
                closeOnCancel: false
            }, function (isConfirm) {
                if (isConfirm) {
                    GetJsonDataFromUrl('/Student/RequestTranskript', {}, function (res) {
                    if (res.result) {
                        MesajSuccess(res.message, "İşlem Başarılı")
                    } else {
                        MesajWarning(res.message, "İşlem Başarısız")
                    }
                })
                }
                swal.close();
            });
            },
        }
    }

    $(document)
        .on("ready", function () {

            $('#fullName').text(myPage.allData.ogrenciBilgi.objects.ad + " " + myPage.allData.ogrenciBilgi.objects.soyad);
            $('#studentNumber').text(myPage.allData.ogrenciBilgi.objects.ogrenci_no);
            $('#emailAddress').text(myPage.allData.ogrenciBilgi.objects.eposta_adresi);

            var sinavProgramiData = [];
            $.each(myPage.allData.sinavProgrami, function (i, item) {
                var key = Object.keys(item)[0];
                var data = item[key];
                sinavProgramiData.push({
                    id: i,
                    start: new Date(data.baslangic_saat),
                    end: new Date(data.bitis_saat),
                    title: key
                });
            })

            myPage.functions.createScheduler("examScheduler", sinavProgramiData);

            myPage.functions.loadSchedule();
            myPage.functions.loadMyLessons();
            myPage.functions.loadNotification();
            myPage.functions.loadExamResult();

            $('body').removeClass('mini-navbar');
        })
</script>

